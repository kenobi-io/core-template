{
    "name": "core-template",
    "version": "1.0.1",
    "description": "The package is intended for common interfaces and functionality that is shared between the frontend and backend is a library agnostic.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kenobi-io/core-template.git"
    },
    "author": "kenobi-io",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kenobi-io/core-template/issues"
    },
    "homepage": "https://github.com/kenobi-io/core-template#readme",
    "keywords": [
        "core",
        "template",
        "core-template",
        "library agnostic"
    ],
    "scripts": {
        "build": "rm -rf dist && tsc",
        "format": "prettier 'src/**/*.ts' --write -c",
        "lint": "eslint 'src/**/*.ts' --quiet --fix --cache --cache-location '.cache/.eslintcache'",
        "version:patch": "npm version patch",
        "version:minor": "npm version minor",
        "version:major": "npm version major",
        "publish": "npm publish",
        "prepare": "husky install",
        "test": "jest --cache"
    },
    "peerDependencies": {
        "rxjs": "^7.5.6"
    },
    "devDependencies": {
        "@types/jest": "28.1.6",
        "@types/node": "^18.7.3",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.3.6",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sort": "^2.4.0",
        "eslint-plugin-sort-class-members": "^1.14.1",
        "husky": "^8.0.1",
        "jest": "28.1.3",
        "prettier": "^2.7.1",
        "ts-jest": "28.0.8",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "4.1.0",
        "typescript": "^4.7.4"
    }
}
