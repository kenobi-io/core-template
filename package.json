{
    "name": "core-template",
    "version": "1.1.1",
    "description": "The package is intended for common interfaces and functionality that is shared between the frontend and backend is a library agnostic.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kenobi-io/core-template.git"
    },
    "author": "kenobi-io",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kenobi-io/core-template/issues"
    },
    "homepage": "https://github.com/kenobi-io/core-template#readme",
    "keywords": [
        "core",
        "template",
        "core-template",
        "library agnostic"
    ],
    "scripts": {
        "build": "rm -rf dist && tsc",
        "format": "prettier 'src/**/*.ts' --write -c",
        "lint": "eslint 'src/**/*.ts' --quiet --fix --cache --cache-location '.cache/.eslintcache'",
        "version:patch": "npm version patch",
        "version:minor": "npm version minor",
        "version:major": "npm version major",
        "publish": "npm publish",
        "prepare": "husky install",
        "test": "jest --cache"
    },
    "peerDependencies": {
        "rxjs": "7.8.0"
    },
    "devDependencies": {
        "@types/jest": "29.4.0",
        "@types/node": "18.14.5",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "@typescript-eslint/parser": "5.54.0",
        "eslint": "8.35.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsdoc": "40.0.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-sort": "2.4.0",
        "eslint-plugin-sort-class-members": "1.16.0",
        "husky": "8.0.3",
        "jest": "29.4.3",
        "prettier": "2.8.4",
        "ts-jest": "29.0.5",
        "ts-loader": "^9.4.2",
        "ts-node": "10.9.1",
        "tsconfig-paths": "3.14.2",
        "typescript": "4.9.5"
    }
}
